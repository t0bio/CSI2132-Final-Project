CREATE TABLE `Appointment` (
  `appointment_id` int NOT NULL,
  `dentist_id` int DEFAULT NULL,
  `patient_id` int DEFAULT NULL,
  `starttime` time DEFAULT NULL,
  `date` date DEFAULT NULL,
  `endtime` time DEFAULT NULL,
  `appointment_type` varchar(25) DEFAULT NULL,
  `status` varchar(25) DEFAULT NULL,
  `room_assigned` int DEFAULT NULL,
  PRIMARY KEY (`appointment_id`),
  KEY `dentist_id` (`dentist_id`),
  KEY `patient_id` (`patient_id`),
  CONSTRAINT `appointment_ibfk_1` FOREIGN KEY (`dentist_id`) REFERENCES `employee` (`employee_id`),
  CONSTRAINT `appointment_ibfk_2` FOREIGN KEY (`patient_id`) REFERENCES `patient` (`patient_id`)
  
